#!/bin/bash
CURRENT_FILE_DIR=$(cd "$( dirname "$0" )" && pwd)
echo $CURRENT_FILE_DIR

java -ea                          \
  -Xms512M                        \
  -Xmx1024M                       \
  -server                         \
  -jar "${CURRENT_FILE_DIR}"/libs/openapi-generator-cli-6.2.1.jar "$@"